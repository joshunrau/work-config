{
  "layer": "top",
  "position": "top",
  "height": 34,
  "margin-right": 16,
  "margin-left": 16,
  "modules-left": ["hyprland/window", "hyprland/workspaces"],
  "modules-right": ["cpu", "memory", "clock"],
  "clock": {
    "format": "{:%A %B %d  %H:%M}"
  },
  "cpu": {
    "format": "CPU: {}%"
  },
  "memory": {
    "format": "Memory: {}%"
  },
  "hyprland/workspaces": {
    "format": "{name}"
  },
  "hyprland/window": {
    "format": "{initialTitle}"
  }
}
